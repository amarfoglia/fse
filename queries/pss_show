PREFIX fse: <https://fse.ontology/>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX tag: <http://www.holygoat.co.uk/owl/redwood/0.1/tags/>
PREFIX api: <http://purl.org/linked-data/api/vocab#>
PREFIX org: <http://www.w3.org/ns/org#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX loinc: <http://purl.bioontology.org/ontology/LNC/>

SELECT ?id ?sect ?title ?body ?time ?code ?performer ?vehicle
FROM <https://fse.ontology/>
WHERE {
  ?id 
    a fse:firstAidReport ;
      fse:hasSection ?sect .
      ?sect fse:hasCode ?code ;
    OPTIONAL {
      ?sect fse:body ?body ;
            fse:title ?title .
    }
    OPTIONAL {
      ?sect fse:mainProblem ?problem .
    }
    OPTIONAL {
      ?sect fse:effectiveTime ?time .
    }
    OPTIONAL {
      ?sect fse:hasPerformer ?performer .
    }
    OPTIONAL {
      ?sect fse:hasVehicle ?vehicle .
    }
}